{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:security/recommended",
        "plugin:jest/recommended",
        "plugin:jest-extended/recommended",
        "plugin:testing-library/react",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "testing-library",
        "jest",
        "jest-extended",
        "security"
    ],
    "rules": {
    }
}
